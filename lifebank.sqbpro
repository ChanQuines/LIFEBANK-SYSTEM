DROP TABLE IF EXISTS &quot;Seeds&quot;;
DROP TABLE IF EXISTS &quot;Locations&quot;;
DROP TABLE IF EXISTS &quot;Transactions&quot;;
DROP TABLE IF EXISTS &quot;users&quot;;</sql><sql name="SQL 3*">CREATE TABLE &quot;users&quot; (
    &quot;user_id&quot; INTEGER NOT NULL,
    &quot;username&quot; TEXT NOT NULL,
    &quot;email&quot; TEXT NOT NULL UNIQUE,
    &quot;password&quot; TEXT NOT NULL,
    &quot;role&quot; TEXT NOT NULL,
    PRIMARY KEY(&quot;user_id&quot; AUTOINCREMENT)
);</sql><sql name="SQL 4*">CREATE TABLE &quot;Locations&quot; (
    &quot;location_id&quot; INTEGER NOT NULL,
    &quot;location_name&quot; TEXT NOT NULL UNIQUE,
    &quot;storage_method&quot; TEXT,
    &quot;contact_person&quot; TEXT,
    PRIMARY KEY(&quot;location_id&quot; AUTOINCREMENT)
);</sql><sql name="SQL 5*">CREATE TABLE &quot;Seeds&quot; (
    &quot;seed_id&quot; INTEGER NOT NULL,
    &quot;common_name&quot; TEXT NOT NULL,
    &quot;variety&quot; TEXT NOT NULL,
    &quot;default_unit&quot; TEXT NOT NULL,
    PRIMARY KEY(&quot;seed_id&quot; AUTOINCREMENT)
);</sql><sql name="SQL 6*">CREATE TABLE &quot;Inventory&quot; (
    &quot;seed_id&quot; INTEGER NOT NULL,
    &quot;location_id&quot; INTEGER NOT NULL,
    &quot;quantity_on_hand&quot; REAL NOT NULL,
    &quot;date_last_counted&quot; TEXT NOT NULL,
    
    -- Ensures a seed/location combination is unique
    PRIMARY KEY(&quot;seed_id&quot;, &quot;location_id&quot;),
    
    FOREIGN KEY(&quot;seed_id&quot;) REFERENCES &quot;Seeds&quot;(&quot;seed_id&quot;) ON DELETE CASCADE,
    FOREIGN KEY(&quot;location_id&quot;) REFERENCES &quot;Locations&quot;(&quot;location_id&quot;)
);</sql><sql name="SQL 7*">CREATE TABLE &quot;Transactions&quot; (
    &quot;transaction_id&quot; INTEGER NOT NULL,
    &quot;item_name&quot; TEXT NOT NULL,
    &quot;quantity_affected&quot; REAL NOT NULL,
    &quot;action_type&quot; TEXT NOT NULL,
    &quot;timestamp&quot; TEXT NOT NULL DEFAULT (DATETIME('now','localtime')),
    &quot;user_executed&quot; TEXT NOT NULL,
    PRIMARY KEY(&quot;transaction_id&quot; AUTOINCREMENT)
);</sql><sql name="SQL 8*">INSERT INTO users(username, email, password, role) 
